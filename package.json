{
  "name": "ReactNativePlayground",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --port 8088 --simulator=\"iPhone 15 Pro Max\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "preinstall": "node ./scripts/pre-install",
